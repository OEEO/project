view {transition: all 0.3s;}

page {
  height: 100%;
  box-sizing: border-box;
  font-size: 20.8333rpx;
  background-color: #f5f5f5;
}

.header {
  width:100%;
  height:96rpx;
  font-size:28rpx;
  background:#fff url(http://img.yummy194.cn/images/logo.png) no-repeat center center / auto 96rpx;
  position: fixed;
  top:0;
  z-index: 99;
}
.header .title{font-size: 35rpx; line-height: 94rpx;text-align: center;}
.header.line{border-bottom: 2rpx solid #eee;}
.header .location {
  background: url(http://img.yummy194.cn/images/homepage_city_icon@2x.png) no-repeat 22rpx center / 48rpx 48rpx;
  padding-left: 74rpx;
  line-height: 96rpx;
  width:140rpx;
}
.header.changeStyle{
  background: linear-gradient(top, rgba(0, 0, 0, 0.4), rgba(0, 0, 0, 0));
  background: -o-linear-gradient(top, rgba(0, 0, 0, 0.4), rgba(0, 0, 0, 0));
  background: -webkit-linear-gradient(top, rgba(0, 0, 0, 0.4), rgba(0, 0, 0, 0));
}
.header .u_shares{
    background: url("http://img.yummy194.cn/images/personal_sousuo_btn@2x.png") no-repeat scroll left 16.6% / 42rpx auto;
    cursor: pointer;
    display: inline-block;
    height: 42rpx;
    position: absolute;
    right: 31rpx;
    top: 27rpx;
    width: 44rpx;
}

.wrapper {
  width:100%;
  position: absolute;
  top:0;
  bottom: 98rpx;
  left:0;
  right:0;
  background: #fff;
}

.the_blank{clear:both;width:100%;margin:0;padding:0;height:25rpx;background:#f5f5f5;display: block;}

.footer{
    bottom: 0;
    left:0;
    right:0;
    height: 98rpx;
    position: absolute;
    z-index: 999;
    background: #fff;
}

.headerBlank{height: 94rpx;display: block;width: 100%;}

.span {display: inline;}
.small {display: inline; font-size: 80%;}
.button {display: inline-block; border:solid 1rpx #ddd; background: #eee;}
.dialogBox{background: rgba(0, 0, 0, 0.5);height: 100%;left: 0;position: absolute;top: 0;width: 750rpx;z-index: 1111;}
.dialogBox .sbox{width: 582rpx;padding-top: 63rpx;border-radius: 4rpx;background: #fff;position: absolute;left: 84rpx;top:50%;transform: translate(0%,-50%);}

.shareBox {position:absolute; z-index:1000; background:rgba(0,0,0,0.6); left:0; right:0; top:0; bottom:0;}
.shareBox .p{width:420rpx; margin-left:304rpx; margin-top:42rpx;margin-bottom:40rpx; font-size:35rpx; background:url(http://img.yummy194.cn/images/web_share_bg@3x.png) no-repeat 0 0 / 420rpx 125rpx; display: block;height:125rpx;
line-height:155rpx;color:#b39851;text-align:center;}
.shareBox .tip{color: #888;display: inline-block;font-size: 26rpx;padding:20rpx 31rpx;}
.shareBox .share_know{width: 200rpx;line-height:83rpx;text-align: center;border-radius: 8rpx;background: #b39851;color: #fff;font-size: 35rpx;float: right;margin-right: 27rpx;display: block;}

.Times,.repotrTimes{width:750rpx; height:100%; background:rgba(0, 0, 0,0.5); position:absolute; top:0; left:0; z-index:1999;}
.Times .xxniu{ display:block; width:38rpx; height:38rpx; background:url(http://img.yummy194.cn/images/course_timelist_close_btn@2x.png) 0 0 / 38rpx 38rpx no-repeat; position:absolute; top:271rpx; left:573rpx; border-radius:50%;}
.Times .time_list,.repotrTimes .report_list{width:500rpx; margin:313rpx auto auto auto; background:#FFF; border-radius:4rpx; overflow:hidden;}
.Times .center_list{width:500rpx; overflow:auto; max-height:500rpx;}
.Times .time_list .time_title{width:100%; line-height:83rpx; text-align:center; color:#333; font-size:29rpx;}
.Times .time_list .list_t{width:100%; height:125rpx; border-top:1rpx solid #d8d8d8;}
.Times .time_list .list_t .t_left{width:80%; padding-top: 13rpx; float:left;}
.Times .time_list .list_t .t_left text{line-height:50rpx; padding-left:21rpx; color:#333; font-size:22rpx; display: block;}
.Times .time_list .list_t .t_left text.top{font-size:25rpx;}
.Times .time_list .list_t .t_right{width:20%; height:125rpx; float:left;}
.Times .time_list .list_t .yes{background:url(http://img.yummy194.cn/images/course_timelist_nochoose_btn@2x.png) center / 33rpx 33rpx no-repeat;}
.Times .time_list .list_t .empty{background:url(http://img.yummy194.cn/images/course_timelist_nochoose_btn@2x.png) center / 33rpx 33rpx no-repeat;}
.Times .time_list .list_t .select{background:url(http://img.yummy194.cn/images/course_timelist_selected_btn@2x.png) center / 33rpx 33rpx no-repeat;}
.Times .time_list .time_but{width:100%; height:73rpx; line-height:73rpx; text-align:center; background:#b39851; font-size:29rpx; color:#FFF;}
.Times .time_list .list_t .t_left.none text{color:#d8d8d8;}

.rcontext{font-size: 28rpx;padding-top: 35rpx;}
.rcontext .rtitle{text-align: center;}
.rcontext textarea{width: 440rpx;border:1rpx solid #eee;margin: 20rpx 20rpx;height: 165rpx;padding: 10rpx;}
.btnss{width: 100%;height: 90rpx;line-height: 90rpx;display: flex;font-size: 31rpx;}
.btnss text{flex: 1;text-align: center;}
.btnss .yes{color: #fff;background: #b39851;}
.btnss .no{color: #888;border-top: 1rpx solid #eee;}

.QRcode{width:750rpx; height:100%; background:rgba(0, 0, 0,0.5); position:absolute; top:0; left:0; z-index:1999;}
.QRcode .views{width:583rpx; height:719rpx;text-align:center;background:#fff;position:relative;z-index:9999;top: 50%;left: 50%;transform:translate(-50%,-50%);-webkit-transform:translate(-50%,-50%);}
.QRcode .views .a{font-size:35rpx;color:#000;height:113rpx;line-height:113rpx;}
.QRcode .views .b{font-size:29rpx;color:#6c6c6c;height:88rpx;line-height:88rpx;margin:0 42rpx;border-top:2rpx solid #e1e1e1;border-bottom:4rpx dashed #e1e1e1;}
.QRcode .views .c{font-size:35rpx;color:#000;margin:25rpx 0;}
.QRcode .views .d{margin:0 auto;margin-bottom:42rpx; height:563rpx; overflow:hidden; position:relative;}
.QRcode .views .d{position:relative; height:563rpx; display:block;}
.QRcode .views .d {float:left; width:583rpx; text-align:center; list-style:none;}
.QRcode .views .d .p{font-size:46rpx; color:#000; width:583rpx; text-align:center; padding-top:52rpx; padding-bottom:25rpx; margin:0;}
.QRcode .views .d .codeimg,.codebox .codeimg{width:360rpx; height:360rpx; display:inline-block;}
.QRcode .views .d .codeimg canvas,.codebox .codeimg canvas{width:360rpx; height:360rpx;}
.codeswiper{height: 500rpx;}

.QRcode .views .L_niu{width:73rpx; height:73rpx; background:url(http://img.yummy194.cn/images/check-code_last_unselect@2x.png) 0 0 / 73rpx 73rpx; position:absolute; top:258rpx; left:25rpx;}
.QRcode .views .L_y{background:url(http://img.yummy194.cn/images/check-code_last_unselect@2x.png) 0 0 / 73rpx 73rpx;}
.QRcode .views .R_niu{width:73rpx; height:73rpx; background:url(http://img.yummy194.cn/images/check-code_next_unselect@2x.png) 0 0 / 73rpx 73rpx; position:absolute; top:258rpx; right:25rpx;}
.QRcode .views .R_y{background:url(http://img.yummy194.cn/images/check-code_next_unselect@2x.png) 0 0 / 73rpx 73rpx;}

.QRcode .views .num{font-size:38rpx;color:#b39851;margin:21rpx auto; margin-bottom:8rpx; width:583rpx; position:absolute; left:0; top:500rpx;}
.QRcode .views .dBtm{font-size:29rpx;color:#cccccc;margin:33rpx auto;}
.QRcode .closed{width:10%;position:absolute;margin-top:25rpx;left:45%;}

