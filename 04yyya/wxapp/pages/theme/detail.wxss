.header{background: linear-gradient(top, rgba(0, 0, 0, 0.4), rgba(0, 0, 0, 0));background: -moz-linear-gradient(top, rgba(0, 0, 0, 0.4), rgba(0, 0, 0, 0));background: -o-linear-gradient(top, rgba(0, 0, 0, 0.4), rgba(0, 0, 0, 0));background: -webkit-linear-gradient(top, rgba(0, 0, 0, 0.4), rgba(0, 0, 0, 0));}
.bodyTop{width:750rpx;}
.bodyTop .top_img {width: 100%;height: 492rpx;}
.bodyTop .top_img image {width:100%;height:492rpx;}
.bodyTop .center_t{width:688rpx; margin:0 auto; margin-top:25rpx;}
.bodyTop .center_t .ct_title{width:100%; line-height:83rpx; text-align:center; font-size:29rpx; color:#333333;}
.bodyTop .center_t .subct{font-size: 25rpx;text-align: center;color:#aaa;width:100%;}
.bodyTop .center_t .ct_time{width:671rpx; line-height:42rpx; text-align:right; color:#888888; font-size:23rpx; padding-right:17rpx;}
.bodyTop .center_t .ct_content{width:688rpx; line-height:42rpx; color:#333333; font-size:25rpx; padding-bottom:63rpx;}

.pro_list{display:block; background-color:#f5f5f5;}
.pro_list .ac_title{width:750rpx; height:104rpx; line-height:138rpx; text-align:center; font-size:29rpx; color:#333;}
.pro_list .product_list{width:100%;}
.pro_list .product_list .li{width:92%; margin:21rpx 4% 58rpx 4%;}
.pro_list .pro_top{width:100%; line-height:42rpx; text-align:left; color:#333333; margin-bottom:21rpx;}
.pro_list .pro_bottom{width:100%; height:183rpx; background-color:#fff; display: -webkit-box; display: -webkit-flex; display: -ms-flexbox; display: flex; box-shadow:4rpx 4rpx 4rpx #eee;}
.pro_list .pro_bottom .ac_img{width:281rpx;height:183rpx; border:none;}
.pro_list .pro_bottom .ac_img image{width:100%; height:100%;}

.pro_list .pro_bottom .ac_title{width:408rpx;height:183rpx;}
.pro_list .pro_bottom .ac_title .ac_t{width:388rpx; line-height:42rpx; height:83rpx; overflow: hidden; font-size:27rpx; color:#333; padding-top:13rpx; text-align:left; padding-left:15rpx; padding-right:6rpx;}
.pro_list .pro_bottom .ac_title .ac_p{width:365rpx; text-align:left; color:#462E2E; font-size:23rpx; padding-left:44rpx; line-height:42rpx; margin-top:31rpx; position:relative; letter-spacing:0rpx;}
.pro_list .pro_bottom .ac_title .ac_p .span{display:inline-block; color:#b39851; font-size:25rpx; line-height:42rpx; position:absolute; right:25rpx; top:2rpx;}
.pro_list .pro_bottom .ac_title .ac_p .ad{ max-width:229rpx; overflow:hidden;text-overflow: ellipsis; white-space: nowrap;}
.pro_list .pro_bottom .ac_title .ac_p:before{content:''; position:absolute; top:7rpx; left:17rpx; width:21rpx; height:25rpx; background:no-repeat url('http://img.yummy194.cn/images/topics_add_icon@3x.png') 0 0 / 21rpx 25rpx;}

.pro_list .split {text-align: center; line-height:104rpx; color:#bbb;font-size: 25rpx;}
.pro_list .passed image {filter: grayscale(100%); -webkit-filter: grayscale(100%); -moz-filter: grayscale(100%); -ms-filter: grayscale(100%); -o-filter: grayscale(100%);}

.pro_list .the_end {text-align: center;}
.pro_list .the_end view.btn {width:375rpx; height:63rpx; border:none; background: #980c10; color:#fff; border-radius: 4rpx; margin: 21rpx; font-size:29rpx;}
