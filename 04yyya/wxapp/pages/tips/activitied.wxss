.awrapper{height: 100%;display: block;}
.pro_list{display:block; background-color:#f5f5f5;}
.pro_list .product_list{width:100%;background: #fff;}
.pro_list .product_list{width:100%; margin-bottom:0; background-color:#fff;}
.pro_list .product_list .p_banner{width:100%; height:167rpx;background:no-repeat url('http://img.yummy194.cn/images/pro_banner.png') 0 0 / auto 167rpx;}
.pro_list .pro_top{width:708rpx; height:94rpx; display: -webkit-box; display: -webkit-flex; display: -ms-flexbox; display: flex; padding:21rpx;display: none;}
.pro_list .User_Img{margin-top:4rpx;margin-right:25rpx;}
.pro_list .User_Img .imgPortrait{width:88rpx;height:88rpx; border:none;}

.pro_list .User_Conten{width:604rpx; display: -webkit-box; display: -webkit-flex; display: -ms-flexbox; display: flex;}
.pro_list .User_Conten .Contens{width:75%;letter-spacing:0rpx; padding-top:4rpx;}
.pro_list .User_Conten .Contens .qUserName{font-size:31rpx;color:#414141;}
.pro_list .User_Conten .Contens .msgDetail{font-size:25rpx;line-height:25rpx;letter-spacing:0rpx;color:#333333;margin:13rpx auto;}
.pro_list .User_Conten .Contens .msgDetail .song_mi{float:left;}
.pro_list .User_Conten .Contens .msgDetail .b_right{padding-right:17rpx; margin-right:17rpx; border-right:4rpx solid #8e8e8e;}
.pro_list .User_Conten .Contens .msgDetail font{color:#8e8e8e;}
.pro_list .User_Conten .User_right{width:25%;}
.pro_list .User_Conten .User_right image{width:123rpx; height:63rpx; margin-top:15rpx;}

.pro_list .pro_center{width:100%; height:429rpx; overflow:hidden; position:relative;}
.pro_list .pro_center image{width:100%;}
.pro_list .pro_center view.span{width:117rpx; height:44rpx; line-height:44rpx; font-size:23rpx; color:#FFF; text-align:center; background:url('http://img.yummy194.cn/images/class.png') no-repeat  0 0 / 117rpx 44rpx; position:absolute;top:46rpx; left:0rpx;}

.pro_list .pro_title{width:688rpx; margin:0rpx auto; display: -webkit-box; display: -webkit-flex; display: -ms-flexbox; display: flex; position:relative;border-bottom:1rpx solid #eee;
}
.pro_list .pro_title .title_left{width:688rpx; max-height:83rpx; line-height:42rpx; font-size:31rpx; color:#414141; padding:31rpx 0; letter-spacing:0rpx; display:inline-block; overflow:hidden;}
.pro_list .pro_title .price_right{line-height:42rpx; font-size:33rpx; color:#b39851; font-weight:600; display:inline-block; position:absolute; top:31rpx; right:0rpx;}

.pro_list .pro_buttom{width:688rpx; height:104rpx; line-height:104rpx; margin:0rpx auto; font-size:25rpx; color:#8c8c8c; display: -webkit-box; display: -webkit-flex; display: -ms-flexbox; display: flex;}
.pro_list .pro_buttom .pro_time{height:42rpx; padding:31rpx 0; line-height:42rpx; text-indent:35rpx; position:relative; display:inline-block; letter-spacing:0rpx;flex: 1;}
.pro_list .pro_buttom .pro_time:before{content:''; position:absolute; top:33rpx; left:0rpx; width:33rpx; height:33rpx; background:url('http://img.yummy194.cn/images/pro_time2.png') no-repeat 0 0 / 33rpx 33rpx;}
.pro_list .pro_buttom .pro_adress{padding:31rpx 0 0;  height:42rpx; line-height:42rpx; text-indent:35rpx; position:relative; overflow:hidden; text-overflow:ellipsis; white-space:nowrap;display:inline-block;}
.pro_list .pro_buttom .pro_adress:before{content:''; position:absolute; top:33rpx; left:0rpx; width:33rpx; height:33rpx; background:url('http://img.yummy194.cn/images/adress.png') no-repeat 0 0 / 33rpx 33rpx;}
.pro_list .pro_buttom .price_right{color: #b39851;padding:60rpx 0 0 ;  height:42rpx; line-height:42rpx;}
.pro_list .pro_buttom .price_right small{font-size: 31rpx;font-weight: bold;}
.pro_list .product_list .pro_line{width: 688rpx; height: 2rpx;background: #eaeaea;margin: 60rpx auto 21rpx;}
.bottom_null{height:94rpx; background:#f5f5f5;  border-top:2rpx solid #eee;}
.pro_list .pro_center .sellout{width:250rpx; height:250rpx; position:absolute; top:121rpx; left:250rpx;}
.pro_list .pro_center .sellout image{width:250rpx; height:250rpx;}