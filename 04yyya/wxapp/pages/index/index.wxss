/** raise-selector **/
.raise-selector_nav {
    height: 84rpx;
    width: 100%;
    background: #fff;
    box-shadow: 0px 2px 4px 0px#ebeae7;
}

.raise-selector_nav .raise-selector {
    position: relative;
    display: flex;
    justify-content: space-around;
/*    padding: 0 50rpx 0 39rpx;*/
    box-sizing: border-box;
    width: 100%;
    height: 100%;
    overflow: hidden;
    text-align: center;
}

.raise-selector_nav .raise-selector .raise-selector-item {
    display: inline-block;
/*    padding: 48rpx 0 24rpx;*/
    /* margin: 0 25rpx; */
    color: #333;
    font-size: 30rpx;
    overflow: hidden;
    line-height:84rpx;
    font-weight:bold;
    width:100%;
}

.raise-selector_nav .raise-selector .raise-selector-item.active {
    color: #b39851;
    border-bottom: 2rpx solid #b39851;
    box-sizing: border-box;
}

