}

.raise-statistics .raise-statistics_item {
    flex: 1;
    text-align: center;
    overflow: hidden;
}

.raise-statistics .raise-statistics_item > text {
    display: block;
    line-height: 1;
}

.raise-statistics .raise-statistics_item > text.sm-text1 {
    text-align: left;
