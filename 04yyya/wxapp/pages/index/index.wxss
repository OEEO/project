.raise-base-info {
    padding: 30rpx 40rpx 16rpx;
    background: #fff;
    margin-bottom: 20rpx;
}


.raise-title {
    font-size: 32rpx;
    color: #000;
    font-weight: bold;
    display: block;
    margin: 0 auto 24rpx;
}

.raise-introduction {
    display: block;
    font-size: 24rpx;
    color: #333;
    margin: 0 auto;
}
.raise-progress {
    /* padding: 0 40rpx; */
    width: 670rpx;
	height: 6rpx;
    background-color: rgba(179, 152, 81, 0.3);
    border-radius: 3rpx;
    margin: 42rpx auto 0;
}

.raise-progress .raise-progress_line {
    height: 100%;
    border-radius: 3rpx;
    opacity: 1;
    background: #b39851;
    position: relative;
    overflow: none !important;
}
.raise-statistics {
    display: flex;
    margin: 20rpx 40rpx 40rpx;
