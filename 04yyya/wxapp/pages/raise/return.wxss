

.raise-return .raise-return_header {
    background: #fff;
    position: fixed;
    top:0;
    left: 0;
    z-index: 100;
    width: 100%;
    font-size: 24rpx;
    text-align: right;
    line-height: 28rpx;
    padding: 24rpx 40rpx;
    box-sizing: border-box;
    border-top: 1rpx solid #eee;
}

.raise-return .raise-return_header image {
    width: 28rpx;
    height: 28rpx;
    margin-left: 10rpx;
    vertical-align: text-top;
}

.raise-return .raise-return_list {
    margin-top: 76rpx;
}

.raise-return .raise-return_list .raise-return_item {
    width: 100%;
    background: #fff;
    padding: 0 40rpx;
    box-sizing: border-box;
    border-top: 20rpx solid #eee;
}

.raise-return .l_title {
    height: 60rpx;
    line-height: 60rpx;
    padding: 36rpx 0;
    border-bottom: 1px solid #eee;
}

.raise-return .l_title image {
    width: 40rpx;
    height: 40rpx;
    margin-right: 18rpx;
    vertical-align: middle;
}

.raise-return .l_title .money {
    font-size:  36rpx;
    position: relative;
    padding-left: 50rpx;    
    display: inline-block;
}

.raise-return .l_title .money:after {
    content: '';
    width: 40rpx;
    height: 40rpx;
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    margin: auto;
    background: transparent url(data:image/png;base64,) no-repeat 100% / 100%;
}

.raise-return .l_title .info {
    float: right;
    font-size: 24rpx;
    color: #999;
}

.raise-return .l_con {
    padding: 50rpx 0;
    border-bottom: 1px solid #eee;
}

.raise-return .raise-return-title {
    font-size: 30rpx;
}

.raise-return .raise-return-content {
    display: block;
    font-size: 24rpx;
    color: #666;
    margin-top: 36rpx;
}

.raise-return .l_foot {
    padding: 46rpx 0;
    color: #666;
    font-size: 24rpx;
}

.raise-return .raise-return-nav_btn {
    color: #fff;
    background: #b39851;
    text-align: center;
    border-radius: 2rpx;
    float: right;
    height: 48rpx;
    line-height: 48rpx;
    padding: 0 12rpx;
}
    
.raise-return .raise-return-nav_btn.disabled {
    color: #999;
    background: #f9f9f9;
}
/* 风险提示对话框 */
.risk-tips-modal-wrap{
  position: fixed;
  top: 0;
  left: 0;
  display: flex;
  z-index: 1000;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 100%;
  background-color: rgba(50, 50, 50, 0.3);
}
.risk-tips-modal{
  box-sizing: border-box;
  position: relative;
  width: 600rpx;
  height: 580rpx;
  border-radius: 4rpx;
  background-color: #fff;
  padding: 80rpx 38rpx 0 58rpx;
}

/* 隐藏滚动条 */

.risk-tips-modal::-webkit-scrollbar {
    width: 0;
    height: 0;
    background-color: transparent;
}
.risk-tips-modal-cancel-icon{
  position: absolute;
  top: 24rpx;
  right: 24rpx;
}
.risk-tips-modal-body{
  height: 492rpx;
}

.risk-tips-modal-header{
  font-size: 32rpx;
  text-align: center;
}
.risk-tips-modal-content{
  font-size: 22rpx;
  line-height: 36rpx;
  margin-top: 17rpx;
}
.risk-tips-modal-confirm{
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  font-size: 30rpx;
  text-align: center;
  background-color: #b39851;
  color: #fff;
}

.risk-tips-modal-body .raise-tag {
    color: #fff;
    height: 48rpx;
    font-size: 24rpx;
    line-height: 48rpx;
    background-color: #b39851;
    padding: 0 12rpx;
    display: inline-block;
}
.risk-tips-modal-body .raise-tag:not(:first-child){
  margin-top: 48rpx;
}


.risk-tips-modal-body .wxParse-p {
    font-size: 24rpx;
    line-height: 48rpx;
    color: #333;
    margin-top: 25rpx;
}

.risk-tips-modal-body .line-p text {
    display: block;
    margin-bottom: 20rpx;
}
