@import "../common/common.wxss";

.address-list .address-item {
    background: #fff;
    margin-bottom: 20rpx;
    box-shadow: 0 0 1px rgba(0,0,0,0.1);
    position: relative;
    padding: 30rpx 40rpx;
    font-size: 28rpx;
    box-sizing: border-box;
}

.address-list .address-item .address-item_info {
    display: inline-block;
    width: 536rpx;
}

.address-list .address-item .select-btn {
    position: absolute;
    top: 0;
    bottom: 0;
    right: 40rpx;
    margin: auto;
    width: 40rpx;
    height: 40rpx;
    background: transparent url(data:image/png;base64,) no-repeat 100% /100%;
    background-size: cover;
}

.address-list .address-item .select-btn.active {
    background: transparent url(data:image/png;base64,) no-repeat 100% / 100%;
    background-size: cover;
}

.new-btn {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 92rpx;
    line-height: 92rpx;
    border-radius: 0;
    text-align: center;
    font-size: 36rpx;
    color: #B39851;
    border: none;
    background: #fff;
}

button[class="new-btn"]::after {
  border: 0;
} 