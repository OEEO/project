.header{background: #fff;}
.orderOperation{display: inline-block; position: absolute; right: 31rpx;top: 0; color:#b39851; font-size:35rpx;line-height: 94rpx;}
/***************************************************/
.the_blanking{background: #f5f5f5 none repeat scroll 0 0; clear: both; height:38rpx; margin: 0; padding: 0;}

.takePhotos{height:167rpx; width:167rpx; margin:0 auto; border-radius:50%; box-shadow:0 0 6rpx #999; position:relative;}
.takePhotos image{width:100%; height:100%; border-radius:50%;}
.takePhotos navigator{display:block; width:73rpx; height:73rpx; border-radius:50%; position:absolute; bottom:-10rpx; right:-10rpx; background:url(http://img.yummy194.cn/images/personaldata_photography_btn@3x.png) no-repeat 0 0 / 73rpx 73rpx;}
/***************************************************/
.userInf{font-size:31rpx;color:#585858;display:block;position:relative;background:#fff;}
.userInf .li{display:block; padding:0 38rpx;height:104rpx;line-height:104rpx; color:#5D5D5D; position:relative;border-top:1px solid #eee;}
.userInf .li input,.userInf .li .telephone{font-size:29rpx;float:right;margin:23rpx 0 0 0;max-width:563rpx;line-height:58rpx; height:58rpx; text-align:right;}
.userInf .li .telephone {color:#999;margin: 0;}
.userInf .li.tel {line-height:42rpx; padding: 27rpx 38rpx; border-bottom:solid 1px #eee;}
.userInf .li.tel small {color:#bbb; font-size: 22rpx;display: block;}
.userInf navigator {border-top:solid 1rpx #eee; border-bottom:solid 1rpx #eee;}

.userInf .li .citys,.userInf .li .provinces,.userInf .li .sexs{border:solid 1rpx rgba(220,220,220,0.3); border-radius: 10rpx;}
.userInf .li .provinces:after,
.userInf .li .citys:after,
.userInf .li .sexs:after{content: url('http://img.yummy194.cn/images/pulldown_L_normal@2x.png'); position: absolute; width: 25rpx;height: 25rpx; line-height:60rpx; right:10rpx; color:#ddd; font-size:21rpx; top: 0;}
.userInf .li .m, .userInf .li div .d {min-width: 63rpx;}
.userInf .li .province:after,
.userInf .li .city:after,
.userInf .li .sex:after,
.userInf .li .y:after,
.userInf .li .m:after,
.userInf .li .d:after {content: "▼"; position: absolute; line-height:58rpx; right:10rpx; color:#ddd; font-size:21rpx; }

.userInf .cao {width:42rpx; height:104rpx; margin-top:0rpx; background:url("http://img.yummy194.cn/images/personaldata_next_icon@3x.png") no-repeat scroll 0 31rpx / 21rpx 42rpx; float:right;}
.bottom_null{width:750rpx; height:354rpx; background:#f5f5f5;}

.userInf .default{text-align:left;}

.userInf navigator{display:block; padding:0 38rpx;height:104rpx;line-height:104rpx; color:#5D5D5D;}
.border_aqi{ border-bottom:1px solid #eee; width:100%; padding: 58rpx 0 42rpx 0; background: #fff;}
/*******自定义的 radio 样式 结束********/

.userInf .li div label{margin-left:42rpx;color:#a3a3a3;}
.userInf .li div input{appearance:radio;-moz-appearance:radio; -webkit-appearance:radio;}

.head_pic_box{ position:absolute; top:0; left:0; bottom:0; right:0; background:rgba(0,0,0,0.4); z-index:1111;}
.head_pic_box a{position:absolute; display:block; width:125rpx; height:125rpx; border-radius:50%; top:167rpx; left:313rpx;}
.head_pic_box a img{width:125rpx; height:125rpx; border-radius:50%;}
.head_pic_box a.add_border_select{border:4rpx solid #fff;}
/***********结束************/
.userInf .li .provinces,.userInf .li .citys,.userInf .li .sexs{color: #999;min-width:104rpx; height:58rpx; text-align:center; line-height:58rpx; font-size:25rpx; border:solid 1rpx rgb(221, 221, 221); border-radius: 10rpx; display: inline-block; padding:0 33rpx 0 4rpx; position: relative;float: right;margin: 23rpx 0;}